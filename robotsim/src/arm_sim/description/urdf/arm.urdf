    <limit effort="411" lower="-6.2832" upper="6.2832" velocity="3.1416"/>
    <dynamics damping="60.0" friction="4.0" />
  </joint>

  <link name="link3">
    <visual>
      <geometry>
        <cylinder length="0.4" radius="0.05"/>
